  "type": "module",
  "scripts": {
    "createFile": "node ./src/fs/create",
    "copyFile": "node ./src/fs/copy",
    "renameFile": "node ./src/fs/rename",
    "removeFile": "node ./src/fs/delete",
    "listFile": "node ./src/fs/list",
    "readFile": "node ./src/fs/read",
    "envFile": "RSS_USER_NAME='user name' RSS_USER_SURNAME='user surname' node ./src/cli/env",
    "argvFile": " node  ./src/cli/args --propName='value1' --propName='value2'",
    "cjsToEsm": "node ./src/modules/cjsToEsm.mjs",
